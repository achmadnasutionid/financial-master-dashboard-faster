{
  "name": "master-dashboard-faster",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "test:ui": "dotenv -e .env.test -- vitest --ui",
    "test:critical": "dotenv -e .env.test -- vitest run tests/critical",
    "test:setup": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:reset": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "test:cleanup": "tsx scripts/cleanup-test-data.ts",
    "test:check": "tsx scripts/check-test-data.ts",
    "test:cleanup-leaked": "node cleanup-test-data.mjs",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:studio:test": "dotenv -e .env.test -- prisma studio",
    "db:push": "prisma db push",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-pdf/renderer": "^4.3.1",
    "@tiptap/extension-placeholder": "^3.18.0",
    "@tiptap/react": "^3.18.0",
    "@tiptap/starter-kit": "^3.18.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ioredis": "^5.9.2",
    "lucide-react": "^0.562.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "pg": "^8.18.0",
    "prisma": "^5.22.0",
    "react": "19.2.1",
    "react-datepicker": "^9.0.0",
    "react-dom": "19.2.1",
    "react-number-format": "^5.4.4",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0"
